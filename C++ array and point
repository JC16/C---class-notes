
#include <stdio.h>
#include "header.h"

int main(int argc, const char * argv[])
{

    // insert code here...
    printf("Hello, World!\n");
    
    int data[5]={7,19,31,67,93};
    PRI("data", data);
    PRI("data[2]", data[2]);
    
    int *ptr=data;
    PRI("ptr", ptr);
    PRI("*ptr", *ptr);
    PRI("*ptr+1", *ptr+1);
    PRI("*(ptr+1)", *(ptr+1));
    PRI("data[1]", data[1]);
    
    
    
    return 0;
}
