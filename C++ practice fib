
#include <stdio.h>

int fib(int x)
    {
        if(x==1 || x==2)
        {
            return 1;
        }
        else
        {
            return fib(x-1)+fib(x-2);
        }
    }
int main(int argc, const char * argv[])
{

    // insert code here...
    
    printf("fib(1)=%d\n",fib(1));
    printf("fib(2)=%d\n",fib(2));
    printf("fib(3)=%d\n",fib(3));
    printf("fib(6)=%d\n",fib(6));
    printf("fib(10)=%d\n",fib(10));
    return 0;
}
